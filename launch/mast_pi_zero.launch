<?xml version="1.0"?>
<launch>
  <arg name="name" default="camera"/>
  <group ns="$(arg name)">
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>
    <node name="raspicam" pkg="mast_pi_zero" type="raspicam_node"/>
    <node name="main" pkg="mast_pi_zero" type="main_node"/>
  </group>
</launch>
